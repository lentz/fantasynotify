{
  "name": "fantasynotify",
  "version": "1.0.0",
  "description": "Transaction notifications for Yahoo Fantasy Leagues",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lentz/fantasynotify.git"
  },
  "author": "Jesse Lentz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lentz/fantasynotify/issues"
  },
  "homepage": "https://github.com/lentz/fantasynotify#readme",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit --watch",
    "check-leagues": "node ./dist/checkLeagues.js",
    "lint": "eslint '**/*.ts'",
    "prebuild": "rm -rf ./dist",
    "start": "node ./dist/app.js",
    "serve": "tsnd --rs --inspect -- app.ts",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "body-parser": "^1.20.0",
    "client-oauth2": "^4.3.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^6.0.6",
    "got": "^11.8.3",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "mongoose": "^6.6.3",
    "mongoose-encryption": "^2.1.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "babel-jest": "^29.0.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "jest": "^29.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "restoreMocks": true,
    "setupFiles": [
      "./jestSetup.ts"
    ],
    "testEnvironment": "node"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
