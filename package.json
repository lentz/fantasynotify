{
  "name": "fantasynotify",
  "version": "1.0.0",
  "description": "Transaction notifications for Yahoo Fantasy Leagues",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lentz/fantasynotify.git"
  },
  "author": "Jesse Lentz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lentz/fantasynotify/issues"
  },
  "homepage": "https://github.com/lentz/fantasynotify#readme",
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc --watch",
    "check-leagues": "node ./dist/checkLeagues.js",
    "lint": "eslint '**/*.ts'",
    "prebuild": "rm -rf ./dist",
    "start": "node ./dist/app.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "engines": {
    "node": "^14.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.1",
    "body-parser": "^1.19.2",
    "client-oauth2": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^6.0.2",
    "got": "^11.8.3",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.2",
    "mongoose": "^6.2.9",
    "mongoose-encryption": "^2.1.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "restoreMocks": true,
    "setupFiles": [
      "./jestSetup.ts"
    ],
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "watch": "dist",
    "nodeArgs": "--inspect"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
