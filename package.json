{
  "name": "fantasynotify",
  "version": "1.0.0",
  "description": "Transaction notifications for Yahoo Fantasy Leagues",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lentz/fantasynotify.git"
  },
  "author": "Jesse Lentz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lentz/fantasynotify/issues"
  },
  "homepage": "https://github.com/lentz/fantasynotify#readme",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit --watch",
    "check-leagues": "node ./dist/checkLeagues.js",
    "lint": "eslint '**/*.ts'",
    "prebuild": "rm -rf ./dist",
    "start": "node ./dist/app.js",
    "serve": "tsx watch app.ts",
    "test": "vitest run",
    "test-watch": "vitest"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "body-parser": "^1.20.2",
    "client-oauth2": "^4.3.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^7.0.4",
    "got": "^12.6.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "mongoose": "^6.8.2",
    "mongoose-encryption": "^2.1.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vitest": "^0.0.57",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3",
    "vitest": "^0.29.8"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
