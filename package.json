{
  "name": "fantasynotify",
  "version": "1.0.0",
  "description": "Transaction notifications for Yahoo Fantasy Leagues",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lentz/fantasynotify.git"
  },
  "author": "Jesse Lentz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lentz/fantasynotify/issues"
  },
  "homepage": "https://github.com/lentz/fantasynotify#readme",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit --watch",
    "check-leagues": "node ./dist/checkLeagues.js",
    "lint": "biome check --error-on-warnings",
    "prebuild": "rm -rf ./dist",
    "prepare": "husky",
    "start": "node ./dist/app.js",
    "serve": "node --watch --inspect=:9229 --env-file .env app.ts",
    "test": "vitest run",
    "test-watch": "vitest"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "client-oauth2": "^4.3.3",
    "express": "^5.1.0",
    "express-handlebars": "^8.0.3",
    "handlebars": "^4.7.7",
    "helmet": "^8.1.0",
    "mongoose": "^8.15.1",
    "mongoose-encryption": "^2.1.2",
    "morgan": "^1.10.0",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/express": "^5.0.2",
    "@types/morgan": "^1.9.9",
    "husky": "^9.1.7",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
